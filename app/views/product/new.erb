<h1>new product</h1>

<%= form_for :product do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
         
    <p>
        <%= f.label :version%><br>
        <%= f.text_area :version %>
    </p>

    <p>
        <%= f.label :description%><br>
        <%= f.text_area :description %>
    </p>
                     
    <p>
      <%= f.submit %>
    </p>
<% end %>
